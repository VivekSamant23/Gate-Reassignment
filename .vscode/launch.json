{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Backend: Init DB (init_data.py)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/init_data.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Backend: Run API (app.py)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/app.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "DATABASE_URL": "sqlite:///gate_reassignment.db"
      }
    }
  ]
}